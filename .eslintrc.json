{
    "env": {
        "node": true,
        "jest": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": ["airbnb-base", "prettier"],
    "plugins": ["prettier"],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console": "off",
        "prettier/prettier": "error",
        "global-require": 0,
        "import/no-dynamic-require": 0
    }
}
